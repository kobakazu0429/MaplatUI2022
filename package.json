{
  "name": "@maplat/ui",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "tsm": "typed-scss-modules src --implementation sass --nameFormat none --exportType default",
    "tsm:watch": "yarn tsm --watch",
    "build:icons": "node bin/icons.mjs",
    "build:scss": "node bin/build-css.mjs",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "yarn lint:scss --fix",
    "dev": "npm-run-all build:icons -p tsm:watch storybook",
    "build": "npm-run-all -s build:icons build:scss tsm",
    "lost-pixel:run": "lost-pixel",
    "lost-pixel:update": "lost-pixel update"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@ergosign/storybook-addon-pseudo-states-html": "^0.0.1-alpha.59",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/html": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/testing-library": "^0.0.11",
    "@types/hammerjs": "^2.0.41",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "fantasticon": "^1.2.3",
    "lost-pixel": "^2.17.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "postcss-pseudo-classes": "https://github.com/philippone/postcss-pseudo-classes.git#90932a5",
    "postcss-scss": "^4.0.4",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "stylelint-config-standard-scss": "^4.0.0",
    "svgo": "^2.8.0",
    "typed-scss-modules": "^6.5.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "clean-css": "^5.3.0",
    "destyle.css": "^3.0.2",
    "hammerjs": "^2.0.8",
    "just-throttle": "^4.0.1",
    "swiper": "^8.2.6",
    "ts-pattern": "^4.0.3"
  }
}
